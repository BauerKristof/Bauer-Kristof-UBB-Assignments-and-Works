<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainMenu" maxHeight="600.0" maxWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button fx:id="createDB" layoutX="54.0" layoutY="106.0" mnemonicParsing="false" onAction="#createDatabase" text="Create Database">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="210.0" layoutY="37.0" text="Main menu">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="createTable" layoutX="54.0" layoutY="162.0" mnemonicParsing="false" onAction="#createTable" prefHeight="30.0" prefWidth="122.0" text="Create Table">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="modifyBtn" layoutX="53.0" layoutY="222.0" mnemonicParsing="false" onAction="#allModifications" prefHeight="26.0" prefWidth="122.0" text="Modify" />
      <Button fx:id="insertDB" layoutX="54.0" layoutY="278.0" mnemonicParsing="false" onAction="#loadinsertDB" prefHeight="30.0" prefWidth="122.0" text="Insert">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="deleteDB" layoutX="53.0" layoutY="337.0" mnemonicParsing="false" onAction="#loadDeleteDB" prefHeight="30.0" prefWidth="122.0" text="Delete">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
